    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "jira-prepare-commit-msg": "^1.6.2",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.3.0",
    "text-encoding": "^0.7.0",
    "ts-jest": "^27.1.5",
    "typescript": "^4.6.3",
    "yargs": "^17.7.2"
  },
  "jira-prepare-commit-msg": {
    "messagePattern": "[$J]\n$M",
    "jiraTicketPattern": "([A-Z]+-\\d+)",
    "commentChar": "#",
    "isConventionalCommit": true,
    "allowEmptyCommitMessage": false
  }
