{
  "name": "api-backend",
  "version": "0.1.0",
  "description": "",
  "main": "dist/src/start.js",
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage --forceExit",
    "build": "tsc && npm run postbundle",
    "execute":"node dist/src/start.js",
    "build-dev": "nodemon -e ts --exec \"clear && echo \"Loading...\" && tsc --incremental && npm run postbundle && clear && node dist/src/start\" ",
    "dev": "nodemon -e ts --exec \"clear && echo \"Loading...\" && clear && ts-node src/start\" ",
    "postbundle": "copyfiles src/core/config/swagger_routes/*.yml dist/ && copyfiles src/core/config/swagger_definitions/*.yml dist/  && copyfiles src/core/config/certs/*.pem dist/",
    "migrate": "ts-node ./script/migration.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.316.0",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@types/qs": "^6.9.7",
    "@types/supertest": "^2.0.12",
    "aws-sdk": "^2.1362.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-list-routes": "^1.1.3",
    "is-plain-object": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "mongoose": "^6.3.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "node_smart_logger": "^3.13.0",
    "nodemailer": "^6.8.0",
    "npx": "^10.2.2",
    "openapi-types": "^12.0.2",
    "oracledb": "^5.5.0",
    "qs": "^6.11.0",
    "ramda": "^0.29.0",
    "sequelize": "^6.17.0",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^8.5.8",
